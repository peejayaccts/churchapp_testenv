'use strict';

/**
 * @ngdoc function
 * @name appsApp.components.people:PeopleService
 * @description
 * # PeopleSrvc
 * Controller of the appsApp
 */

angular.module('appsApp',[])
  .factory('dataFactory',['$http', function($http) {
        return $http.get('http://127.0.0.1:8000/api/people/?format=json').success(function(data){
            return data;
        })
        .error(function(err){
            return err;
        });

  }]); 
